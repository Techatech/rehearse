{
  "name": "rehearse",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "LiquidMetal AI app",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest",
    "create-migration": "npx ts-node ./scripts/create_migration.mts",
    "clean-seed": "find ./db -name '*seed*' -type f -delete",
    "seed:createSql": "npx ts-node ./scripts/seed-sql.mts",
    "render-db": "pnpm run clean-seed && pnpm run create-migration && pnpm run seed:createSql"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@liquidmetal-ai/in-memory-d1": "^1.0.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/markdown-it": "^14.1.2",
    "better-sqlite3": "^11.9.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prisma": "^6.5.0",
    "prisma-kysely": "^1.8.0",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.7.0",
    "vitest": "^3.1.3",
    "zx": "^8.5.2"
  },
  "dependencies": {
    "@cerebras/cerebras_cloud_sdk": "^1.56.1",
    "@liquidmetal-ai/raindrop-framework": "^0.10.0",
    "@modelcontextprotocol/sdk": "^1",
    "@workos-inc/node": "^7.72.2",
    "axios": "^1.13.2",
    "kysely": "^0.27.2",
    "kysely-d1": "^0.3.0",
    "mammoth": "^1.11.0",
    "markdown-it": "^14.1.0",
    "pdfjs-serverless": "^1.1.0",
    "pizzip": "^3.2.0",
    "stripe": "^19.3.1",
    "zod": "^3"
  }
}